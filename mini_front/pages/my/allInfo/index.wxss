/* pages/my/allInfo/index.wxss */

.status {
  padding-top: 80rpx;
  padding-bottom: 60rpx;
  height: 200rpx;
  background-color: #000;
  background: url(data:image/png;base64,) no-repeat center;
  background-size: 27%;
}

.noPass {
  display: inline-block;
  width: 30rpx;
  height: 33rpx;
  position: relative;
  top: 10rpx;
  left: 25rpx;
}

.titleMes {
  text-align: center;
}

.timeTitle {
  font-size: 40rpx;
  color: #000;
}

.timesBox {
  padding: 20rpx;
  margin: 0px 55rpx 0rpx 55rpx;
}

.times {
  font-size: 60rpx;
  color: #333;
}

.status_msg {
  margin-top: 25rpx;
  color: #ff5a8e;
  font-size: 28rpx;
}

/* 表格 */

.giveClass {
  padding: 100rpx 78rpx 78rpx 78rpx;
}

.personClass {
  margin-bottom: 100rpx;
  text-align: center;
  font-size: 30rpx;
  color: #333;
  border-radius: 4px;
  border: solid 1px #e7e7e7;
  border-bottom: 0;
}

.personTitle {
  height: 78rpx;
  line-height: 78rpx;
  border-bottom: solid 1px #e7e7e7;
}

/* 培训信息 */

.classMes {
  font-size: 28rpx;
}

.classTitle {
  color: #888;
}

.classInfo {
  color: #333;
  font-size: 28rpx;
}

.mesBox {
  margin-bottom: 30rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.wx_pay {
  text-align: center;
  height: 78rpx;
  line-height: 78rpx;
  color: #fff;
  background: #09bb07;
  position: fixed;
  bottom: 0;
  width: 100%;
}

.mon {
  width: 23rpx;
  height: 33rpx;
  vertical-align: middle;
  margin-left: 10rpx;
}
.btnFix{
  position:fixed;
  bottom:0;
  width:100%;
  text-align:center;
  border-radius:0 !important;
}
