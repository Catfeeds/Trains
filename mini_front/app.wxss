@import "/wxParse/wxParse.wxss";

.clearBoth {
  clear: both;
  overflow: hidden;
}

.left {
  float: left;
}

.right {
  float: right;
}

.img {
  width: 100%;
  height: 295rpx;
}

.border {
  padding-left: 20rpx;
  border: 1px solid #ccc;
  height: 80rpx;
  line-height: 80rpx;
  font-weight: 300;
}

.lineblock {
  display: inline-block;
}

.flex {
  display: -webkit-box;
  display: -ms-flexbox;
  display: flex;
  -ms-flex-pack: distribute;
  justify-content: space-around;
}

.one {
  -webkit-box-flex: 1;
  -ms-flex: 1;
  flex: 1;
  font-weight: 300;
}

.hide {
  display: none !important;
}

.hr {
  height: 20rpx;
  background-color: rgb(233, 229, 229);
}

.block {
  display: block;
}

.textCenter {
  text-align: center;
}

.textEllipsis {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

/* button[disabled][type=primary] {
  background: #1aad19 !important;
} */

/* 图标 */

.success {
  background-image: url(data:image/png;base64,) !important;
}

.error {
  background-image: url(data:image/png;base64,) !important;
}

.wait {
  background-image: url(data:image/png;base64,) !important;
}

.addStudy {
  padding-left: 43%;
  line-height: 119rpx;
  color: #ff5a8e;
  border-bottom: 1px solid #ccc;
  background: url(data:image/png;base64,) no-repeat 33% center;
  background-size: 7%;
}

/* 学员列表 */

.stuInfo {
  width: 378rpx;
}

.sex {
  padding-left: 10rpx;
  display: inline-block;
  width: 21rpx;
  height: 31rpx;
}

.userPhone {
  font-size: 26rpx;
  color: #999;
}

.userName {
  font-size: 32rpx;
  color: #333;
  margin-right: 25rpx;
}

.boy {
  width: 18rpx;
  height: 26rpx;
}

.girl {
  width: 11rpx;
  height: 30rpx;
}

.studentBox {
  padding: 20rpx 60rpx 20rpx 0rpx;
  border-bottom: 1px solid #ccc;
}

.studentBox:last-child {
  border-bottom: 0;
}

.students {
  margin-left: 40rpx;
  padding-bottom: 90rpx;
}

/* 模拟单选框 */

.checkedBg {
  line-height: 40rpx;
  width: 40rpx;
  height: 80rpx;
  background: url(data:image/png;base64,) no-repeat center;
  background-size: 100%;
}

.checkBg {
  line-height: 40rpx;
  width: 40rpx;
  height: 80rpx;
  background: url(data:image/png;base64,) no-repeat center;
  background-size: 100%;
}

/* 选中按钮默认样式修改 */

radio .wx-radio-input.wx-radio-input-checked {
  border: 0;
  background: url(data:image/png;base64,) no-repeat !important;
  background-size: 100% !important;
}

radio .wx-radio-input.wx-radio-input-checked::before {
  display: none;
}

/* 输入框样式 */

.parkTitle-left {
  width: 212rpx;
  display: inline-block;
  float: left;
}

.parkTitle-right {
  width: 65%;
  color: #333;
}

.parkBox {
  margin: 20rpx 0;
}

.textContract {
  height: 68rpx;
  line-height: 68rpx;
}

.park {
  padding: 30rpx;
  padding-left: 60rpx;
  font-size: 30rpx;
  color: #666;
  font-weight: 300;
}

.clear:after {
  content: '';
  display: block;
  clear: both;
}

.clear {
  zoom: 1;
}

/* 男女性别 */

.boy {
  background: url(data:image/png;base64,) no-repeat center;
  background-size: 100%;
}

.girl {
  background: url(data:image/png;base64,) no-repeat center;
  background-size: 100%;
}
